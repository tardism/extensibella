Define $greatereq_integer : integer -> integer -> bool -> prop by
  $greatereq_integer N1 N2 B := $lesseq_integer N2 N1 B.

%unique
Theorem extensibella-$-stdLib-$-greatereq_integer_unique : forall N1 N2 B B',
  $greatereq_integer N1 N2 B -> $greatereq_integer N1 N2 B' -> B = B'.
intros. case H1. case H2. backchain extensibella-$-stdLib-$-lesseq_integer_unique.

%total
Theorem extensibella-$-stdLib-$-greatereq_integer_total : forall N1 N2,
  is_integer N1 -> is_integer N2 ->
  exists B, $greatereq_integer N1 N2 B.
intros. apply extensibella-$-stdLib-$-lesseq_integer_total to H2 H1. search.

%creates $is_bool
Theorem extensibella-$-stdLib-$-greatereq_integer_is_bool : forall N1 N2 B,
  $greatereq_integer N1 N2 B -> is_bool B.
intros. case H1. apply extensibella-$-stdLib-$-lesseq_integer_is_bool to H2. search.

%other things?

Theorem extensibella-$-stdLib-$-eq_to_greatereq_integer : forall N,
  is_integer N -> $greatereq_integer N N $btrue.
intros. apply extensibella-$-stdLib-$-eq_to_lesseq_integer to H1. search.

Theorem extensibella-$-stdLib-$-greater_to_greatereq_integer : forall N1 N2,
  $greater_integer N1 N2 $btrue -> $greatereq_integer N1 N2 $btrue.
intros. case H1. apply extensibella-$-stdLib-$-less_to_lesseq_integer to H2. search.

Theorem extensibella-$-stdLib-$-greatereq_integer__add_positive :
  forall N1 N2 Base Sum,
    $greatereq_integer N1 Base $btrue ->
    $greatereq_integer N2 ($posInt $zero) $btrue ->
    $plus_integer N1 N2 Sum -> $greatereq_integer Sum Base $btrue.
intros G1 G2 Plus. case G1. case G2.
apply extensibella-$-stdLib-$-lesseq_integer__add_positive to H1 H2 Plus. search.


