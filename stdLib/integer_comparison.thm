  $lesseq_integer ($posInt $zero) ($posInt ($succ N2)) $btrue;
  $lesseq_integer ($posInt ($succ N1)) ($posInt $zero) $bfalse;
  $lesseq_integer ($posInt ($succ N1)) ($posInt ($succ N2)) B :=
